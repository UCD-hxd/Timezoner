{
  "name": "Timezoner",
  "version": "1.0",
  "description": "Getting start example",
  "permissions": ["storage", "declarativeContent","activeTab"],
  "background": {
    "scripts": ["background.js","popup.js"],
    "persistant": false
  },

  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/get_started16.png",
      "32": "icons/get_started32.png",
      "48": "icons/get_started48.png",
      "128": "icons/get_started128.png"
    }
  },

  "icons": {
    "16": "icons/get_started16.png",
    "32": "icons/get_started16.png",
    "48": "icons/get_started16.png",
    "128": "icons/get_started16.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
